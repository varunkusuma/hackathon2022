{
  "name": "@ecp/ovc-visits-ui",
  "version": "1.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --ssl --open --port 4201",
    "start:lcl": "ng serve --configuration local --ssl --open",
    "start:dev": "ng serve --configuration dev --ssl --open",
    "build": "ng build",
    "test": "ng test --browsers=Chrome",
    "test:watch": "ng test --watch=true --code-coverage",
    "e2e": "webdriver-manager update --no-gecko --no-chrome --ignore_ssl && ng e2e --no-webdriver-update",
    "e2e:lcl": "webdriver-manager update --no-gecko --ignore_ssl && ng e2e --no-webdriver-update --protractor-config e2e/protractor.conf.lcl.js",
    "build:single-spa:ovc-visits-ui": "ng build ovc-visits-ui --prod --deploy-url https://localhost:4200/",
    "serve:single-spa:ovc-visits-ui": "ng s --project ovc-visits-ui --disable-host-check --port 4200 --deploy-url https://localhost:4200/ --live-reload false"
  },
  "repository": {
    "url": "https://github.optum.com/ecp/ovc-visits-ui"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^11.2.13",
