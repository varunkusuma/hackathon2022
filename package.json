    "@ecp/telehealth-angular-ui-component-library": "1.34.0",
    "@uitk/angular": "^2.4.0",
    "angular-circle-counter": "^1.1.0",
    "angular-user-idle": "^2.2.5",
    "apollo-angular": "^2.4.0",
    "apollo-angular-link-http": "^1.11.0",
    "apollo-link": "^1.2.14",
    "apollo-utilities": "^1.3.4",
    "compare-versions": "^4.1.3",
